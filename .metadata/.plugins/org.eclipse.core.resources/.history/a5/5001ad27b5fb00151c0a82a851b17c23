window.onload = function() {
	var chart1 = new CanvasJS.Chart("chart1", {
		colorSet : 'colorSet1',
		title : {
			text : "原始"
		},
		data : data1()
	});
	chart.render();

	var chart2 = new CanvasJS.Chart("chart2", {
		colorSet : 'colorSet1',
		title : {
			text : "平滑"
		},
		data : data2()
	});
	chart.render();

