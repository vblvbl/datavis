window.onload = function () {
	CanvasJS.addColorSet("blueShades",
            [//colorSet Array
            "#2F4F4F",
            "#008080",
            "#2E8B57",
            "#3CB371",
            "#90EE90"                
            ]);
	
    var chart = new CanvasJS.Chart("chart1",
    {
      colorSet:'blueShades',
      title:{
        text: "未优化"
      },   
      data: data1()
    });
    chart.render();
  }